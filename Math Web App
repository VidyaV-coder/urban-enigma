    private void jButton1MouseClicked(java.awt.event.MouseEvent evt) {                                      
        try {
            // TODO add your handling code here:
            Scanner sc = new Scanner(new File("HellO.txt"));
            String user = jTextField1.getText();
            String pwd = new String (jPasswordField1.getPassword());
            boolean Login = false;
            while (sc.hasNextLine()) {
                if (user.equals(sc.nextLine()) && pwd.equals(sc.nextLine())) {
                    this.dispose();
                    new CSWEBAPP().setVisible(true);
                    Login = true;
                }
            }
            if (Login == false) {
            JOptionPane.showMessageDialog(null,"login failure");   
            }    
        } catch (FileNotFoundException ex) {
            Logger.getLogger(LoginScreen.class.getName()).log(Level.SEVERE, null, ex);
        }  
    }                                     

    private void jButton1KeyPressed(java.awt.event.KeyEvent evt) {                                    
        // TODO add your handling code here:
        try {
            // TODO add your handling code here:
            Scanner sc = new Scanner(new File("HellO.txt"));
            String user = jTextField1.getText();
            String pwd = new String (jPasswordField1.getPassword());
            boolean Login = false;
            ArrayList<String> loginIds = new ArrayList<String>();
            while (sc.hasNext()) {
                loginIds.add(sc.nextLine());
            }
            System.out.println(loginIds);
        ArrayList<ArrayList<String>> lIds = new ArrayList<ArrayList<String>>();
        int size = loginIds.size();
        for (int i = 0; i < size; i++) {
            String [] s  = loginIds.get(i).split(" ");
            ArrayList<String> row = new ArrayList<String>(Arrays.asList(s));
            
            lIds.add(row);
        }
            
            for (int i = 0; i < size; i++) {
                if (user.equals(lIds.get(i).get(0)) && pwd.equals(lIds.get(i).get(1)))  {
                    new CSWEBAPP().setVisible(true);
                    Login = true;
                }
            }
            if (Login == false) {
                JOptionPane.showMessageDialog(null,"Login Failure. Try Again");
            }    
        } catch (FileNotFoundException ex) {
            Logger.getLogger(LoginScreen.class.getName()).log(Level.SEVERE, null, ex);
        }  
    }                                   

    private void jButton1MouseEntered(java.awt.event.MouseEvent evt) {                                      
        // TODO add your handling code here:
        try {
            // TODO add your handling code here:
            Scanner sc = new Scanner(new File("HellO.txt"));
            String user = jTextField1.getText();
            String pwd = new String (jPasswordField1.getPassword());
            boolean Login = false;
            ArrayList<String> loginIds = new ArrayList<String>();
            while (sc.hasNext()) {
                loginIds.add(sc.nextLine());
            }
            System.out.println(loginIds);
        ArrayList<ArrayList<String>> lIds = new ArrayList<ArrayList<String>>();
        int size = loginIds.size();
        for (int i = 0; i < size; i++) {
            String [] s  = loginIds.get(i).split(" ");
            ArrayList<String> row = new ArrayList<String>(Arrays.asList(s));
            
            lIds.add(row);
        }
            
            for (int i = 0; i < size; i++) {
                if (user.equals(lIds.get(i).get(0)) && pwd.equals(lIds.get(i).get(1)))  {
                    new CSWEBAPP().setVisible(true);
                    Login = true;
                }
            }
