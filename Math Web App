    private void jButton1MouseClicked(java.awt.event.MouseEvent evt) {                                      
        // TODO add your handling code here:
        DefaultTableModel model = (DefaultTableModel) studentTable.getModel();
        studentTable.setModel(model);
        Object[] row = returnUserInfo();
        writeToFile(row,model);
        jTabbedPane1.setSelectedIndex(0);
    }                                     
    public Object[] returnUserInfo(){
        Object row[] = new Object[4];
        row[0] = jTextField5.getText();
        row[1] = jTextField6.getText();
        row[2] = jTextField7.getText();
        row[3] = jTextField8.getText();
        return row;  
    }
    
    public  void writeToFile(Object[] row, DefaultTableModel model){
        try {
            PrintWriter writer = new PrintWriter(new FileWriter("studentData.txt",true));
            for (int i = 0; i < row.length; i++) {
                writer.print(row[i] + " ");
            }
            writer.println(" ");
            writer.close();
        } catch (FileNotFoundException ex) {
            Logger.getLogger(AddStudent.class.getName()).log(Level.SEVERE, null, ex);
        } catch (IOException ex) {
            Logger.getLogger(AddStudent.class.getName()).log(Level.SEVERE, null, ex);
        }
        
        model.addRow(row);
        
    }
    
